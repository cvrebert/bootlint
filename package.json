    "url": "https://github.com/twbs/bootlint.git"
  },
  "license": "MIT",
  "dependencies": {
    "binary-search": "^1.2.0",
    "bluebird": "^3.0.5",
    "body-parser": "^1.14.1",
    "chalk": "^1.1.1",
    "cheerio": "^0.20.0",
    "commander": "^2.9.0",
    "debug": "^2.1.1",
