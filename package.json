    "glob": "^7.0.3",
    "morgan": "^1.5.1",
    "semver": "^5.0.3",
    "void-elements": "^2.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "grunt": "^0.4.5",
    "grunt-banner": "^0.6.0",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-nodeunit": "^1.0.0",
    "grunt-contrib-qunit": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^20.0.0",
    "jquery": "^3.1.0",
    "jscoverage": "^0.6.0",
    "load-grunt-tasks": "^3.1.0",
    "nodeunit": "^0.9.0",
    "rewire": "^2.3.1",
    "sinon": "^1.12.2",
    "time-grunt": "^1.0.0"
  },
  "main": "./src/bootlint.js",
  "bin": {
    "bootlint": "./src/cli-main.js"
  },
  "browser": {
    "binary-search": false,
    "cheerio": "jquery",
    "url": "./src/url.js",
    "./src/cli.js": false,
    "./src/cli-main.js": false,
    "./src/location.js": false
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "bin",
    "dist",
    "src",
    "app.js",
    "LICENSE"
  ],
  "scripts": {
    "test": "grunt test",
    "coveralls": "jscoverage src && BOOTLINT_COV=1 nodeunit --reporter=lcov test | coveralls",
    "start": "./bin/www"
  }
}
